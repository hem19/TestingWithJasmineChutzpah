{
  "Compile": {
    "Mode": "External",
    "Extensions": [ ".ts" ],
    "ExtensionsWithNoOutput": [ ".d.ts" ]
  },
  "References": [
    {
      "Includes": [ "*/app/*.ts" ],
      "Excludes": [ "*/app/*.d.ts" ]
    }
  ],
  "Tests": [
    {
      "Includes": [ "*/test/*.spec.ts" ],
      "Excludes": [ "*/test/*.d.ts" ]
    }
  ]
}